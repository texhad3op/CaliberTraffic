<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns:wicket>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<title>EventEditPage</title>
<link rel="stylesheet" type="text/css" href="style.css" />
</head>
<body>
<wicket:extend>
	<table border="0">
		<tr>
			<td width="50%" valign="top">
			<form wicket:id="form" name="editForm">
			<table border="1">
				<tr>
					<td>Pavadinimas</td>
					<td><input type="text" wicket:id="deviceNr" /></td>
				</tr>
				<tr>
					<td>Apra≈°ymas</td>
					<td><textarea wicket:id="description"></textarea></td>
				</tr>				
				<tr>
					<td>Nominal</td>
					<td><input type="text" wicket:id="measurementNominal" /></td>
				</tr>
				<tr>
					<td>Tolerancija</td>
					<td><input type="text" wicket:id="measurementTolerance" /></td>
				</tr>
				<tr>
					<td>Minimalus</td>
					<td><input type="text" wicket:id="minCaliber" /></td>
				</tr>
				<tr>
					<td>Maximalus</td>
					<td><input type="text" wicket:id="maxCaliber" /></td>
				</tr>
				<tr>
					<td>Vieta</td>
					<td><input type="text" wicket:id="holdPlace" /></td>
				</tr>
				<tr>
					<td>Griovelis</td>
					<td><input type="text" wicket:id="holdSubPlace" /></td>
				</tr>
				<tr>
					<td>Ar ilgas?</td>
					<td><input type="checkbox" wicket:id="isLongType" /></td>
				</tr>
				<tr>
					<td>Ar patikrintas?</td>
					<td><input type="checkbox" wicket:id="isActive" /></td>
				</tr>
				<tr>
					<td>Data</td>
					<td><input type="text" wicket:id="nextCheckingTime" /></td>
				</tr>
				<tr>
					<td><input type="submit" value="save" /></td>
				</tr>
			</table>
			</form>
			</td>
		</tr>
	</table>

</wicket:extend>
</body>
</html>
